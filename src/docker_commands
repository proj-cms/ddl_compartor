docker cp src\db\db2_admin.sql ddl_comparator-oracle-db2-1:/tmp/db2_sample_tables.sql


docker exec -it ddl_comparator-oracle-db2-1 bash

sqlplus SYSTEM/pass2@localhost/XE
sqlplus SYSTEM/pass2@localhost/XE @/tmp/db2_admin.sql
sqlplus SYSTEM/pass2@localhost/XE @/tmp/db2_sample_tables.sql
